<!DOCTYPE html>
<html>
  <head>
    <title>Assign2</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <% if flash[:notice] %>
      <div id="notice"><%= flash[:notice] %></div>
    <%end%>
    <% if flash[:alert] %>
      <div id="alert"><%= flash[:alert] %></div>
    <%end%>
    <%= yield %>
  </body>
</html>
